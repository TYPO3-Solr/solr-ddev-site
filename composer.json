{
    "name": "dkd/apache-solr-for-typo3-dev-distribution",
    "description": "TYPO3 CMS Base Distribution",
    "license": "GPL-2.0-or-later",
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "repositories": {
        "0": {
            "type": "path",
            "url": "packages/*"
        }
    },
    "require": {
        "ext-json": "*",
        "ext-pdo": "*",
        "helhum/typo3-console": "^6.3.0",
        "typo3/cms-composer-installers": "^3.0.1",
        "typo3/minimal": "^10.4",
        "typo3/cms-about": "^10.4",
        "typo3/cms-adminpanel": "^10.4",
        "typo3/cms-belog": "^10.4",
        "typo3/cms-beuser": "^10.4",
        "typo3/cms-felogin": "^10.4",
        "typo3/cms-fluid-styled-content": "^10.4",
        "typo3/cms-form": "^10.4",
        "typo3/cms-impexp": "^10.4",
        "typo3/cms-info": "^10.4",
        "typo3/cms-lowlevel": "^10.4",
        "typo3/cms-redirects": "^10.4",
        "typo3/cms-reports": "^10.4",
        "typo3/cms-rte-ckeditor": "^10.4",
        "typo3/cms-setup": "^10.4",
        "typo3/cms-seo": "^10.4",
        "typo3/cms-scheduler": "^10.4",
        "typo3/cms-sys-note": "^10.4",
        "typo3/cms-t3editor": "^10.4",
        "typo3/cms-tstemplate": "^10.4",
        "typo3/cms-viewpage": "^10.4",
        "typo3/cms-filemetadata": "^10.4",
        "typo3-console/composer-auto-commands": "^0.4.2",
        "typo3-console/composer-typo3-auto-install": "^0.4",
        "dkd/apache-solr-for-typo3-sitepackage": "@dev",
        "wikimedia/composer-merge-plugin": "^1.4.1",
        "phpdocumentor/reflection-docblock": "5.1.*"
    },
    "autoload-dev": {
        "psr-4": {
            "TYPO3\\CMS\\Core\\Tests\\": "public/typo3/sysext/core/Tests/"
        }
    },
    "scripts": {
        "pre-dependencies-solving": [
            "[ -d public/typo3conf/ext ] ||  mkdir -p public/typo3conf/ext",
            "[ -L public/typo3conf/ext/solr ] ||  ln -snf ../../../packages/ext-solr public/typo3conf/ext/solr"
        ],
        "typo3-cms-scripts": [
            "typo3cms install:fixfolderstructure",
            "typo3cms install:generatepackagestates",
            "typo3cms cache:flush"
        ],
        "post-autoload-dump": [
            "@typo3-cms-scripts"
        ]
    },
    "extra": {
        "merge-plugin": {
            "require": [
                "packages/ext-solr/composer.json"
            ],
            "recurse": false,
            "replace": false,
            "ignore-duplicates": false,
            "merge-dev": true,
            "merge-extra": false,
            "merge-extra-deep": false,
            "merge-scripts": false
        }
    }
}
